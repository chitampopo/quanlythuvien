@model IEnumerable<CoHoiCuoiCung.Models.CHUC_VU>

@{
    ViewBag.Title = "Chức vụ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Chức vụ</h2>

<p>
    @Html.ActionLink("Thêm mới chức vụ", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Tên chức vụ
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TEN_CHUC_VU)
        </td>
        <td>
            @Html.ActionLink("Cập nhật", "Edit", new { id=item.MA_CHUC_VU })
        </td>
    </tr>
}

</table>
